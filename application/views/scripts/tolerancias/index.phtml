<div class="calc"><script language="javascript">

function onlyNumbers(evt)
{
    var e = event || evt; // for trans-browser compatibility
    var charCode = e.which || e.keyCode;

    if (charCode > 31 && (charCode < 45 || charCode > 57))
        return false;

    return true;

}


function DoMath()
{
	var tiragem        = document.frmData.tiragem.value;
	var tolerancia     = document.frmData.tolerancia.value / 100;
	var aproveitamento = document.frmData.aproveitamento.value;
	var subtotal       = (tiragem*(1+tolerancia))/aproveitamento;
	var cores          = document.frmData.cores.value;
	var estragos       = document.frmData.estragos.value;
	var total          =  parseInt(document.frmData.subtotal.value) +  parseInt(document.frmData.estragos.value);

document.frmData.subtotal.value = Math.round(subtotal);

document.frmData.total.value = Math.round(total);
}
function DoMath1()
{
var tiragem1        = document.frmData1.tiragem1.value;
var tolerancia1     = document.frmData1.tolerancia1.value / 100;
var aproveitamento1 = document.frmData1.aproveitamento1.value;
var subtotal1       = (tiragem1*(1+tolerancia1))/aproveitamento1;
var cores1          = document.frmData1.cores1.value;
var estragos1       = document.frmData1.estragos1.value;
var total1          =  (parseInt(document.frmData1.subtotal1.value) +  parseInt(document.frmData1.estragos1.value)) /2;

document.frmData1.subtotal1.value = Math.round(subtotal1);

document.frmData1.total1.value = Math.round(total1);
}

</script>
